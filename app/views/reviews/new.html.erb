<div class="container">
  <h1>New review for <%= @restaurant.name %></h1>
</div>
<div class="container">
  <div class="row">
    <div class="column">
      <%= simple_form_for [ @restaurant, @review] do |f| %>
        <%= f.input :content %>
        <%= f.input :rating, collection: Review::RATING %> 
        <br>
        <br>
        <%= f.submit class: "btn btn-outline-primary" %>
      <% end %>
      </div>
   </div>
</div>